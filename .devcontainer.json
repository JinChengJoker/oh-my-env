{
  "build": {
    "dockerfile": "Dockerfile"
  },
  "runArgs": [
    "--name=oh-my-env"
  ],
  "mounts": [
    "source=repos,target=/root/repos,type=volume",
    "source=haprepos,target=/root/haprepos,type=volume",
    "source=ssh,target=/root/.ssh,type=volume"
  ],
  "customizations": {
    "vscode": {
      "extensions": [
        "vue.volar",
        "vue.vscode-typescript-vue-plugin"
      ]
    }
  }
  // "forwardPorts": []
}